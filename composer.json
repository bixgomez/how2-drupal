{
    "name": "suny-buffalo/however-drupal",
    "description": "Drupal 10 project",
    "type": "project",
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^10.2",
        "drupal/core-recommended": "^10.2",
        "cweagans/composer-patches": "^1.7",
        "drush/drush": "^12.4.3",
        "drupal/pdf": "^1.1",
        "drupal/admin_toolbar": "^3.1",
        "drupal/search_api": "^1.25",
        "drupal/facets": "^2.0",
        "drupal/workbench": "^1.3",
        "drupal/field_group": "^3.2",
        "drupal/paragraphs": "^1.14",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/cer": "^5.0@dev",
        "drupal/search_api_page": "^1.0@beta",
        "drupal/twig_tweak": "^3.3",
        "drupal/better_exposed_filters": "^6.0",
        "drupal/footnotes": "^4.0@alpha",
        "drupal/sendgrid_integration": "^2.0",
        "drupal/layout_builder_component_attributes": "^2.1",
        "drupal/layout_builder_modal": "^1.1",
        "drupal/backup_migrate": "^5.0",
        "drupal/ckeditor_indentblock": "^1.0@beta",
        "drupal/ckeditor5_font": "^1.1@beta",
        "drupal/colorbutton": "^1.5",
        "drupal/ckeditor_font": "^1.4",
        "drupal/classy": "^1.0",
        "drupal/color": "^1.0",
        "drupal/rdf": "^2.1",
        "drupal/fakeobjects": "^2.0",
        "drupal/entity_reference_modal": "^1.0"
    },
    "require-dev": {
        "drupal/core-dev": "^10.2",
        "drupal/upgrade_status": "^4.3"
    },
    "conflict": {
            "drupal/drupal": "*"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[project-root]/.editorconfig": false,
                "[project-root]/.gitattributes": false
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"],
            "web/modules/custom/{$name}": ["type:drupal-custom-module"],
            "web/profiles/custom/{$name}": ["type:drupal-custom-profile"],
            "web/themes/custom/{$name}": ["type:drupal-custom-theme"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": false,
        "platform": {
            "php": "8.1"
        },
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "tbachert/spi": true
        }
    }
}
